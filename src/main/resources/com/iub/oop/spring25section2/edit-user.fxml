<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iub.oop.spring25section2.EditUserController">
    <children>
        <Button layoutX="418.0" layoutY="332.0" mnemonicParsing="false" onAction="#logOut" prefHeight="54.0" prefWidth="168.0" text="Log Out" textAlignment="CENTER" wrapText="true">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" prefHeight="54.0" prefWidth="79.0" text="←" textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="Calibri" size="24.0" />
            </font>
        </Button>
        <Label layoutX="206.0" layoutY="14.0" text="Edit User">
            <font>
                <Font size="48.0" />
            </font>
        </Label>
      <TextField fx:id="usernameTF" layoutX="188.0" layoutY="108.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordTF" layoutX="188.0" layoutY="168.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button layoutX="259.0" layoutY="266.0" mnemonicParsing="false" onAction="#update" text="Update">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ComboBox fx:id="userTypeCB" layoutX="225.0" layoutY="224.0" prefWidth="150.0" promptText="Select User Type" />
    </children>
</AnchorPane>
